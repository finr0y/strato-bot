{
  "name": "strato-bot",
  "version": "1.0.0",
  "description": "This is the bot for the official Strato Emulator Discord Server",
  "type": "module",
  "main": "main.js",
  "scripts": {
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "start": "tsc --build && node build/main.js",
    "tscw": "tsc-watch --onSuccess \"node ./build/main.js\"",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strato-emu/strato-bot.git"
  },
  "author": "https://github.com/strato-emu/strato-bot/contributors",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/strato-emu/strato-bot/issues"
  },
  "homepage": "https://github.com/strato-emu/strato-bot#readme",
  "dependencies": {
    "@octokit/rest": "^19.0.5",
    "@types/node-fetch": "^2.6.2",
    "@types/ws": "^8.5.4",
    "adm-zip": "^0.5.10",
    "discord.js": "^14.8.0",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "tsc-watch": "^6.0.0",
    "typescript": "^4.9.4"
  }
}
